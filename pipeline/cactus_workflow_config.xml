<cactus_workflow_config>
	<alignment>
		<blast_misc overlapSize="10000" chunksPerJob="1"
			compressFiles="True" />
		<iterations>
			<iteration number="0" max_sequence_size="10e100" type="blast">
				<blast chunkSize="100000000"
					blastString="lastz --format=cigar --notransition --seed=14of22 --step=100 --nogapped --hspthresh=6000 SEQ_FILE_1[multiple][nameparse=darkspace] SEQ_FILE_2[nameparse=darkspace] > CIGARS_FILE"
					selfBlastString="lastz --format=cigar --notransition --seed=14of22 --step=100 --nogapped --hspthresh=6000 SEQ_FILE[nameparse=darkspace] --self > CIGARS_FILE" />
				<core annealingRounds="5" alignRepeatsAtRound="0"
					trim="5" trimChange="-0.5" 
					minimumTreeCoverage="0.0" 
					minimumBlockLength="5" minimumBlockLengthChange="0"
					minimumChainLength="50" minimumChainLengthChange="-10"
					deannealingRounds="10"
					adjacencyComponentOverlap="2" />
			</iteration>

			<iteration number="1" max_sequence_size="30000000" type="blast">
				<blast chunkSize="3000000"
					blastString="lastz --format=cigar SEQ_FILE_1[multiple][nameparse=darkspace] SEQ_FILE_2[nameparse=darkspace] > CIGARS_FILE"
					selfBlastString="lastz --format=cigar SEQ_FILE[nameparse=darkspace] --self > CIGARS_FILE" />
				<core annealingRounds="5" alignRepeatsAtRound="3"
					trim="3" trimChange="0" 
					minimumTreeCoverage="0.0" 
					minimumBlockLength="0" minimumBlockLengthChange="0"
					minimumChainLength="200" minimumChainLengthChange="0"
					deannealingRounds="10"
					adjacencyComponentOverlap="3" />
			</iteration>

			<iteration number="2" max_sequence_size="1000000" type="blast">
				<blast chunkSize="1000000"
					blastString="lastz --format=cigar --hspthresh=1800 SEQ_FILE_1[multiple][nameparse=darkspace] SEQ_FILE_2[nameparse=darkspace] > CIGARS_FILE"
					selfBlastString="lastz --format=cigar --hspthresh=1800 SEQ_FILE[nameparse=darkspace] --self > CIGARS_FILE" />
				<core annealingRounds="3" alignRepeatsAtRound="0"
					trim="0" trimChange="0"
					minimumTreeCoverage="0.0" 
					minimumBlockLength="0" minimumBlockLengthChange="0"
					minimumChainLength="100" minimumChainLengthChange="0"
					adjacencyComponentOverlap="0" />
			</iteration>

			<iteration number="3" spanning_trees="10" max_sequence_size="2000" gap_gamma="0.5" use_banding="1" banding_limit="1000000" banding_size="1000" banding_threshold="0.7" type="base" />
		</iterations>
	</alignment>

	<normal rounds="2" max_number_of_chains="30">
	</normal>

	<phylogeny>
	</phylogeny>

	<faces>
	</faces>

	<reference matching_algorithm="blossom5">
	</reference>

	<check time="1">
	</check>
</cactus_workflow_config>
